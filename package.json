{
  "devDependencies": {
    "code-surfer": "^3.0.0-beta.1",
    "d3": "5.12.0",
    "d3-scale": "^3.1.0",
    "gatsby-theme-mdx-deck": "^3.0.13",
    "gh-pages": "^2.1.1",
    "http-server": "^0.11.1",
    "mdx-deck": "^3.0.9",
    "raw-loader": "^3.1.0",
    "react-helmet": "^5.2.1",
    "rxjs": "^7.0.0-alpha.0",
    "website-pdf": "3.0.8"
  },
  "name": "rxjs-practice-makes-perfect",
  "scripts": {
    "serve": "http-server -p 8008 --gzip",
    "start": "mdx-deck index.mdx",
    "test": "echo \"there are no tests\"",
    "build": "mdx-deck  build index.mdx --basePath=\"/practice-makes-perfect/\" && rm -rf docs && mv public docs",
    "export:pdf": "website-pdf http://localhost:8000/print -o deck.pdf",
    "export:image": "npx capture-website-cli http://localhost:8000 --output=deck.png",
    "publish-deck": "gh-pages -d docs"
  },
  "version": "1.0.0"
